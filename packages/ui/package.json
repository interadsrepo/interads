  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "pnpm run type-check --watch",
    "prebuild": "rimraf lib tsconfig.build.tsbuildinfo && pnpm run type-check",
    "build": "pnpm run build:node && pnpm run build:stable && pnpm pnpm run build:umd && pnpm run build:types",
    "build:types": "node ../../scripts/buildTypes.mjs",
    "build:node": "node ../../scripts/build.mjs node",
    "build:stable": "node ../../scripts/build.mjs stable",
    "build:umd": "cross-env BABEL_ENV=stable rollup -c scripts/rollup.config.js"
  },
  "peerDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
